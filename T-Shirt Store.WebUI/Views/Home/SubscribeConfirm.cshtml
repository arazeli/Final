@model CommandJsonResponse

@{
    ViewData["Title"] = "SubscribeConfirm";
}

@if (Model.Error)
{
<div class="alert alert-danger text-white d-flex justify-content-center" role="alert">

    @Model.Message
</div>
}
else
{

<div class="alert alert-success text-white d-flex justify-content-center" role="alert">
    @Model.Message <a asp-action="Index" asp-controller="home" class="alert-link">Ana Sehife</a>.
</div>

}